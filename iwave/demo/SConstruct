import os
thispath = os.getcwd()
env = Environment();

def makefigs(target, source, env):
    os.system('psimage n1=361 f2=0 d2=0.005 f1=0 d1=0.005 label1="depth (km)" label2="offset (km)" legend=1 lstyle=horibottom units="velocity (km/s)" d1num=0.5 width=6 height=4 wrgb=1.0,1.0,1.0 grgb=1.0,0.0,0.0 brgb=0,0,1.0 < $IWAVEPP/demo/maodel/data/marm20m_bulkmod > fig1.ps')

env.Command(['fig1.ps'], ['SConstruct'], makefigs)

