from rsfbook import *
from rsftex import *

Papers(copyr='Madagascar Community',
       group='Madagascar Documentation',
       papers=Split('''
       rsf/paper.tex
       rsf/install.tex
       rsf/prog.tex
       rsf/format.tex
       rsf/tour.tex
       rsf/api.tex
       rsf/demo.tex
       rsf/adding.tex
       scons/paper.tex
       '''),
       authors={'scons':'Sergey Fomel and Gilles Hennenfent'},
       line='\\url{http://rsf.sourceforge.net/}',
       dir='rsf/sfmath',fig='rose',size='width=0.5\\textwidth')
       
for file in ('toc','tpg'):
    Paper(file,lclass='georeport')

End(lclass='georeport',use='hyperref,color,amsmath,fancybox,jfc',
    include='\\newcommand{\\RSF}{../../..}')

