from rsfproj import *
import string

Fetch('Txx.HH','septour')

Result('Wiggle1','Txx.HH','wiggle')

Flow('Txx_windowed','Txx.HH','window n2=10 n1=200 f1=200')

plotpar = ' transp=y poly=y yreverse=y pclip=100'

for program in ('wiggle','contour nc=200','grey'):
    Result(string.capitalize(program),'Txx_windowed',program + plotpar)

Flow('resamp','Txx.HH',
     '''
     window j1=4 | fft1 | pad n1=521 |
     fft1 inv=y | window n1=1024 | scale dscale=4
     ''')

Result('nmo','Txx_windowed',
       'nmostretch v0=2.05 half=n | wiggle pclip=100 max1=0.6 poly=y')

plotpar = plotpar + ' min1=.4 max1=.8 max2=1. min2=.05 poly=n'

Plot('grey','Txx_windowed','grey' + plotpar)
Plot('wiggle1','Txx_windowed','wiggle plotcol=0 plotfat=10' + plotpar)
Plot('wiggle2','Txx_windowed','wiggle plotfat=5' + plotpar)

Result('overplot','grey wiggle1 wiggle2','Overlay')
Result('sidebyside','grey wiggle2','SideBySideIso')

End()
