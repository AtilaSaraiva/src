from rsfproj import *

Flow(['7ab','7c','7d'],None,
     '''7ab top=3.2 c1=.9 c2=6.8
     c=${TARGETS[1]} d=${TARGETS[2]}
     ''',prefix=plprefix)
Plot('7ab','vppen xscale=.57 yscale=1.2')
Plot('7c','grey wanttitle=n')
Plot('7d','grey wanttitle=n')
Plot('7cd','7c 7d','SideBySideAniso')
Result('dip','7ab 7cd','OverUnderAniso')

###########################################################################
End()
