from rsfproj import *

Fetch('shots.hh','shots')

Flow('cube','shots.hh',
     '''
     dd data_format=native_float |
     window n1=400 n2=24 |
     byte
     ''')

cubeplot = '''
grey3 frame1=320 frame2=3 frame3=20 point1=.75
label1="Time (s)" label2=Offset label3=Shot wanttitle=n 
'''

Plot('flat','cube',cubeplot)
Plot('cube','cube',cubeplot + ' flat=n') 
Result('cube','flat cube','SideBySideAniso')

###########################################################################
End()
