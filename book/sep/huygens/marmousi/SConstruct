from rsfproj import *

Fetch('marmvel.hh','marm')

Flow('velocity-raw','marmvel.hh','dd form=native | window j1=2 j2=2 f2=1000 n2=501')
Flow('velocity-smooth','velocity-raw','smooth rect1=5 rect2=5 repeat=2')

for case in ('raw','smooth'):
    Result('velocity-'+case,
           '''
           grey title="Marmousi Model: %s Velocity" allpos=y bias=1000.0
           label1=Depth unit1=m label2=Distance unit2=m
           ''' % ('True','Smoothed')[case=='smooth'])



End()
