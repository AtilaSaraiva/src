import os

os.environ['PSTEXPENOPTS'] = 'color=y force=n fat=1 fatmult=1.5'

from rsftex import *

Paper('seg2005',lclass='segabs')

Paper('gilles')

Paper('tury')

Command2('paper-revised.tex','paper.tex','cp $SOURCE $TARGET')

Paper('paper-revised',options='manuscript,revised')

End(options='manuscript')

