stack.stack('bei',
            v0=1.4,
            nv=48,
            dv=0.025,
            nx=250,
            nt=751,
            padx=521,
            rect1=40,
            rect2=10,
            srect1=1,
            srect2=5,
            vslope=0.67, #
            vx0=1.5,     #
            f1=15,
            nout=2048)

