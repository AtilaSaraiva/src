from rsfproj import *

Fetch('PC203.sgy','gpr')

Flow(['data','tfile','./hfile','./bfile'],'PC203.sgy',
     '''
     segyread tape=$SOURCE
     tfile=${TARGETS[1]} hfile=${TARGETS[2]} bfile=${TARGETS[3]} |
     window f2=1 | tpow tpow=2
     ''',stdin=0)


Fetch('LINE1.sgy','gpr')

Flow(['line1','tfile1','./hfile1','./bfile1'],'LINE1.sgy',
     '''
     segyread tape=$SOURCE
     tfile=${TARGETS[1]} hfile=${TARGETS[2]} bfile=${TARGETS[3]}
     ''',stdin=0)

End()
