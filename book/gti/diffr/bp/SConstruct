from rsfproj import *
import stack

sig = 'signal-3D.H'

Fetch(sig,'bp')
Flow('zof',sig,
     '''
     dd form=native |
     window min1=1 max1=5 min3=-14950 n3=600 n2=1 f2=299 |
     bandpass flo=1 fhi=15 |
     put label1=Time unit1=s label2=Lateral unit2=m
     ''')

Result('zof','grey title="Zero Offset Section" ')

stack.diffimg('bp',
              'zof',
              v0=1400,
              nv=120,
              dv=5,
              nx=600,
              padx=1025,
              nt=667,
              tmin=1.002,
              tmax=4.998,
              rect1=125,
              rect2=25,
              srect1=1,
              srect2=3,
              vslope=None,
              units='m',
              f1=1,
              j3=3,
              dx=50,
              x0=-14950,
              nout=2048,
              vx0=None)

End()
