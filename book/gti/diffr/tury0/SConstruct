stack.stack('tury',
            v0=1400,
            nv=184,
            dv=25,
            x0=2724.9,
            nx=1640,
            nt=1500,
            padx=2049,
            rect1=50,
            rect2=25,
            srect1=1,
            srect2=5,
#            vslope=0.67, #
#            vx0=1.5,     #
            tmax=6,
            f1=15,
            an=2,
            nout=2048)

End()
