from rsfproj import *

Flow('reft',None,
     '''
     spike mag=2
     n1=301 d1=0.01 o1=1
     n2=301 d2=0.01 o2=1
     ''')

Flow('test','reft',
     '''
     kirmod3 nt=101 dt=0.04 vel=2
     nsx=101 dsx=0.05 s0x=0  nhx=1 dhx=0.05 h0x=0
     nsy=101 dsy=0.05 s0y=0 nhy=1 dhy=0.05 h0y=0 |
     window
     ''')

Result('test',
       '''
       byte gainpanel=all |
       grey3 frame1=60 frame2=50 frame3=50 title=Test flat=y
       label2="In-line (km)" label3="Cross-line (km)"
       ''')

End()
