from rsfproj import *

import string

plots = {}
for dist in ('linear',):
    plots[dist] = []
    for appr in ('hyperbolic','malovichko','new'):
        plot = dist[0]+appr[0]
        plots[dist].append(plot)
        Plot(plot,None,
             '''
             approx dist=%s appr=%s |
             grey color=j scalebar=y pclip=100 allpos=y
             transp=n label1="x/xmax" label2="b/a"
             title="%s"
             barlabel="Relative Error (%%)"
             ''' % (dist,appr,string.capitalize(appr)))
    Result(dist,plots[dist],'OverUnderAniso',vppen='txscale=1.5')

End()
