from rsf.proj import *

#------- Define Variables and Filenames ------#
data = 'data.H'

#------------- Import Data -------------------#
#-- Uses ftp program Fetch
Fetch(data ,'pluto')

#----------  Convert Data --------------------#
Flow('shot',data,
     '''
     dd form=native
     ''')

#---------------- Plot Data ---------------#
Result('shot','window j3=20 | grey gainpanel=a')
End()
