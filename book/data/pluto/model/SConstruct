from rsfproj import *

Fetch(['pluto.velo.hh'],'pluto')

Flow('velo','pluto.velo.hh',
     '''
     dd form=native |
     put label1=z unit1=km d1=0.025 o1=0
         label2=x unit2=km d2=0.025 o2=-34.875 |
     math "output=input/1000"
     ''')

# inVelo is a text file containing the header info for velo.rsf. 
Flow('inVelo','velo','in $SOURCE')

Result('velo','grey color=j allpos=y title=Velocity')
End()
