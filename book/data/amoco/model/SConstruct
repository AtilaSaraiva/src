from rsfproj import *
# Fetch Files from repository
Fetch("velsmooth.HH","Amoco")
Fetch("velmodel.hh","Amoco")
# Convert Files to RSF
Flow('velsmooth','velsmooth.HH', '''dd form=native | put
    unit1=km unit2=km label1=Depth\ Z label2=Distance\ X title=Velocity\ Model''')
Flow('velmodel','velmodel.hh', '''dd form=native | put
    unit1=km unit2=km label1=Depth\ Z label2=Distance\ X title=Smoothed\ Velocity\ Model''')
# Plotting Section
#Result('velsmooth','''window $SOURCE  max1=4 | 
#                      grey  color=I gainpanel=a scalebar=y 
#                      allpos=y title=Smoothed\ Velocity\ Model''')
Result('velmodel','''window $SOURCE  max1=4 | grey  scalebar=y
                     color=I gainpanel=a title=Velocity\ Model 
                     allpos=y screenratio=.5 screenht=6
                     scalebar=y barlabel=m/s''')
End()
