from rsfproj import *
# Fetch Files from repository
Fetch("velsmooth.HH","Amoco")
Fetch("velmodel.hh","Amoco")
# Convert Files to RSF
Flow('velsmooth','velsmooth.HH', 'dd form=native')
Flow('velmodel','velmodel.hh', 'dd form=native')
# Plotting Section
Result('velsmooth','''window $SOURCE  max1=4 | 
                      grey  color=I gainpanel=a 
                      title=Smoothed\ Velocity\ Model''')
Result('velmodel','''window $SOURCE  max1=4 | grey  
                     color=I gainpanel=a title=Velocity\ Model''')
End()
