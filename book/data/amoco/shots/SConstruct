from rsfproj import *
# Fetch Files from repository
Fetch("shots.hh","Amoco")
# Convert Files to RSF
Flow('shots','shots.hh', '''dd form=native | 
    put  label1=time label2=offset label3=Shot unit1=km unit2=km unit3=km''')
# Plotting Section
Result('zero','shots','''window $SOURCE  min2=0 max2=0 size2=1 | 
    grey  pclip=96 color=I gainpanel=a title=Zero\ Offset\ Data label2=x''')
Result('shot40','shots','''window $SOURCE  min3=2 max3=2 size3=1 | 
    grey  pclip=99 color=I gainpanel=a title=Shot\ 40''')
End()
