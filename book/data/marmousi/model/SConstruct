from rsfproj import *
# Fetch Files from repository
Fetch("marmsmooth.HH","marm")
Fetch("marmvel.hh","marm")
# Convert Files to RSF and update headers
Flow('marmvel','marmvel.hh','''dd form=native | put
    label1=Depth Z label2=X unit1=m unit2=m''')
Flow('marmsmooth','marmsmooth.HH', '''dd form=native | put 
    label1=Depth Z label2=X unit1=m unit2=m''')
# Plotting Section
Result('marmsmooth','''window $SOURCE  | 
    grey  color=I gainpanel=a allpos=y scalebar=y 
    title=Smoothed\ Velocity\ Model''')
Result('marmvel','''window $SOURCE  | grey  
    color=I gainpanel=a allpos=y scalebar=y
    title=Marmousi\ Velocity\ Model''')
End()

