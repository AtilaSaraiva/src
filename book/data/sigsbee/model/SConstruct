from rsfproj import *

velo = 'sigsbee2a_migvel.sgy'
Fetch(velo,'sigsbee')

Flow('zvelo tzvelo ./vhead ./bvhead',velo,
     '''
     segyread
     tape=$SOURCE
     tfile=${TARGETS[1]}
     hfile=${TARGETS[2]}
     bfile=${TARGETS[3]}
     ''',stdin=0)

Flow('velo','zvelo',
	'''
	put 
	o1=0     d1=25   label1=z unit1=ft
	o2=10025 d2=37.5 label2=x unit2=ft
	''')

Result('velo','grey color=j title=velocity wantscalebar=y allpos=y')

End()
