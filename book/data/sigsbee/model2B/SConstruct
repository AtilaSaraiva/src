from rsfproj import *

## 
 # Sigsbee 2B
 ##

PRFX = 'sigsbee2b_'
SUFX = '.segy' 

for c in ('migration_velocity','stratigraphy'):

    if(c=='migration_velocity'):
        v='vmig'
        o=10025
        d=37.5
    else:
        v='vstr'
        o=10000
        d=25
    
    h = c + 'head'
    t = PRFX + c + SUFX
    Fetch(t,'sigsbee')

    Flow([v,h],t,
         '''
         segyread tape=$SOURCE tfile=${TARGETS[1]} |
         put
         o1=0  d1=25   label1=z unit1=ft
         o2=%f d2=%f   label2=x unit2=ft
         ''' %(o,d) ,stdin=0)

    Result(v,v,'grey color=j allpos=y title=Velocity')

End()
