from rsfproj import *
import string, os

private = {'login':os.environ.get('BEG_LOGIN'),
           'password':os.environ.get('BEG_PASSWORD'),
           'server':os.environ.get('BEG_SERVER')}

for line in (284,288,549):
   line = 'line%d' % line
   for case in ('pp','ps'):
       dat = 'randy_%sstack_%s.sgy' % (string.upper(case),line)
       Fetch(dat,'randy',private)
       rsf = case+line
       Flow([rsf+'-0','t'+rsf,'./h'+rsf,'./b'+rsf],
            dat,
            '''
            segyread tape=$SOURCE
            tfile=${TARGETS[1]} hfile=${TARGETS[2]} bfile=${TARGETS[3]}
            ''',stdin=0)
       Flow(rsf,[rsf+'-0','t'+rsf],'intbin head=${SOURCES[1]} xk=cdp yk=fldr')

End()
