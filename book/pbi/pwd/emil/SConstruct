from rsfproj import *

Fetch('emil2.sht','emil')

Flow('emil','emil2.sht','segyread tape=$SOURCE su=1 endian=0 data=1',stdin=0)
Flow('emil1','emil','window n2=120')
Flow('emil2','emil','window f2=120')

grey = 'grey label1="Time (s)" label2=Trace wanttitle=n'

Plot('emil1','emil1',grey)
Plot('emil2','emil2',grey)
Result('emil','emil1 emil2','SideBySideAniso')
    
###########################################################################
End()
