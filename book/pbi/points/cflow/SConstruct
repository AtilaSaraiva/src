from rsfproj import *

Flow('start',None,
     'math n1=401 n2=401 o1=-2 o2=-2 d1=0.01 d2=0.01 output="x1*x1+x2*x2-2" ')
Flow('next','start','cflow rect=100 band=100')

for case in ('start','next'):
    Plot(case,
         '''
         contour allpos=n nc=3 c0=-1 dc=1 wanttitle=n wantaxis=n
         screenratio=1
         ''')

Result('circle','start next','SideBySideIso')

End()
