from rsfproj import *

# velocity
vstr = 'sigsbee2a_stratigraphy.sgy'
Fetch(vstr,'sigsbee')
Flow('vstr vstr.asc vstr.bin',vstr,
     'segyread hfile=${TARGETS[1]} bfile=${TARGETS[2]} read=d')

# slowness
Flow('sstr','vstr','math output=1/input | transp plane=12 | transp plane=23')

# reflectivity
Flow('ref','vstr','ai2refl | scale axis=12')


End()
