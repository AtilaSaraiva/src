from rsfproj import *

Fetch('beinew.HH','midpts')
Flow('gulf','beinew.HH',
     '''
     dd form=native |
     put
     label1=Time unit1=s
     label2=Half-Offset unit2=km
     label3=Midpoint unit3=km
     ''')

Result('gulf',
       '''
       byte |
       transp plane=23 |
       grey3 flat=n frame1=500 frame2=160 frame3=10 
       title=Data point1=0.8 point2=0.8
       ''')

End()
