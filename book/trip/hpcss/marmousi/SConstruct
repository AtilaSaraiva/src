from rsfproj import *

Fetch('marmvel.hh','marm')

Flow('vel','marmvel.hh','dd form=native')

Flow('trace wave','vel',
     '''
     wave nt=7501 dt=0.0004
     isz=1 isxbeg=1201 isxend=1201 iskip=20
     igz=2 igxbeg=501  igxend=1801 fpeak=25 source=${TARGETS[1]} nm=100
     ''')

Plot('wave','grey gainpanel=all wanttitle=n',view=1)

Result('trace','grey title=Data')

Flow('traceg waveg','vel',
     '''
     wavegeom nt=3751 dt=0.0008
     isz=3 isxbeg=1201 isxend=1201 iskip=20
     igz=3 igxbeg=501  igxend=1801 fpeak=25 source=${TARGETS[1]} nm=100
     ''')

Plot('waveg','grey gainpanel=all wanttitle=n',view=1)

Result('traceg','grey title=Data')


End()
