#!/usr/bin/env python
import sys, os

sys.path.append(os.environ.get('RSFROOT'))
import rsfdoc
from rsfprogs import rsfprog

this = sys.argv.pop(0)

if not sys.argv:
    print "Usage: %s prog1 prog2..." % this
else:
    for prog in sys.argv:
        if not prog.startswith(rsfdoc.rsfprefix):
            prog = rsfdoc.rsfprefix + prog
        if rsfprog.has_key(prog):
            rsfprog[prog].document()
        else:
            print "No program %s in RSF." % prog


    



