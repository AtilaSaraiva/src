#!/usr/bin/env python
import sys, os, re

sys.path.append(os.path.join(os.environ.get('RSFROOT'),'lib'))

import rsfdoc
import rsfprog

this = sys.argv.pop(0)

if not sys.argv:
    print "Usage: %s prog1 prog2..." % this
else:
    for prog in sys.argv:
        if not re.match(rsfdoc.rsfprefix,prog):
            prog = rsfdoc.rsfprefix + prog
        main = rsfdoc.progs.get(prog)
        if main:
            main.document()
        else:
            print "No program %s in RSF." % prog


    



